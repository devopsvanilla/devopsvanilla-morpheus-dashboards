# Copy this file to .env and fill in the values.
# NEVER commit your .env file.

# Morpheus credentials (required)
MORPHEUS_USER=""
MORPHEUS_PASS=""

# OAuth token endpoint (can include query params like client_id, grant_type, scope)
MORPHEUS_TOKEN_URL="https://morpheus-dev.loonar.dev/oauth/token?client_id=morph-api&grant_type=password&scope=write"

# External proxy URL to test (Superset embedded dashboard/page)
MORPHEUS_PROXY_TEST_URL="https://morpheus-dev.loonar.dev/superset/explore/p/Mvj64KlZXbE/?standalone=1&height=400"

# If true, curl will skip TLS verification (-k). Set to false in production.
INSECURE_SSL=true

# NGINX site configuration path for the superset proxy (used for grep display only)
NGINX_SUPERSET_SITE="/etc/nginx/sites-available/superset-proxy"

# Optional: Enable verbose debug output
DEBUG=false
